{
  "name": "beatriz-dono",
  "version": "1.0.0",
  "description": "A Bot multipurpose for discord",
  "main": "index.js",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "lint": "npx @biomejs/biome check --write --diagnostic-level=error",
    "lint:watch": "nodemon --watch . --ext ts,js,cjs,mjs,json --exec 'npm run lint'",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "cp": "npm run cp --workspaces",
    "website:dev": "npm run dev --workspace=packages/website",
    "bot:dev": "env-cmd npm run dev --workspace=packages/bot",
    "bot:build": "env-cmd npm run build --workspace=packages/bot",
    "bot:start": "env-cmd npm run start --workspace=packages/bot",
    "bot:generate_db": "env-cmd npm run generate_db --workspace=packages/bot",
    "docker:prod": "docker compose up --build",
    "docker:dev": "docker compose -f dev.docker-compose.yml up --build"
  },
  "keywords": [],
  "prettier": {
    "printWidth": 120,
    "singleQuote": true,
    "jsxSingleQuote": true,
    "arrowParens": "avoid",
    "semi": false,
    "trailingComma": "none",
    "endOfLine": "crlf",
    "parser": "typescript",
    "tabWidth": 2,
    "useTabs": false
  },
  "endOnLine": "crlf",
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@biomejs/biome": "1.9.2",
    "@types/get-pixels": "^3.3.4",
    "@typescript-eslint/eslint-plugin": "^7.8.0",
    "@vitest/ui": "^1.5.2",
    "env-cmd": "^10.1.0",
    "eslint": "^8.57.0",
    "eslint-config-love": "^47.0.0",
    "eslint-config-prettier": "^9.1.0",
    "nodemon": "^3.1.5",
    "typescript": "^5.4.5",
    "vitest": "^1.5.2"
  },
  "dependencies": {
    "get-pixels": "3.3.3"
  }
}
