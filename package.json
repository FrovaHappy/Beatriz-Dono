{
  "name": "beatriz-dono",
  "version": "1.0.0",
  "description": "A Bot multipurpose for discord",
  "main": "index.js",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "bot:dev": "npm run dev --workspace=packages/bot",
    "bot:build": "npm run build --workspace=packages/bot",
    "bot:start": "npm run start --workspace=packages/bot",
    "bot:generate_db": "npm run generate_db --workspace=packages/bot",
    "bot:deploy_docker": "npm run deploy_docker --workspace=packages/",
    "docker:prod": "docker compose down && docker compose up",
    "docker:dev": "docker compose down && docker compose -f dev.docker-compose.yml --build up"
  },
  "keywords": [],
  "author": "",
  "license": "ISC"
}
