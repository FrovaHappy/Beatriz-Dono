{
  "name": "beatriz-dono",
  "version": "1.0.0",
  "description": "A Bot multipurpose for discord",
  "main": "index.js",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "bot:dev": "env-cmd -f .env npm run dev --workspace=packages/bot",
    "bot:build": "npm run build --workspace=packages/bot",
    "bot:start": "npm run start --workspace=packages/bot",
    "bot:generate_db": "npm run generate_db --workspace=packages/bot",
    "docker:prod": "docker compose up --build",
    "docker:dev": "docker compose -f dev.docker-compose.yml  up --build"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "env-cmd": "^10.1.0"
  }
}
